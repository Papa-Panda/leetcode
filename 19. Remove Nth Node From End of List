# Definition for singly-linked list.
# class ListNode(object):
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution(object):
    def removeNthFromEnd(self, head, n):
        """
        :type head: ListNode
        :type n: int
        :rtype: ListNode
        """
        fast = head
        for i in range(n):
            if fast.next != None: 
                fast = fast.next
            elif i == n-1:
                head = head.next
                return head
            else:
                return None
        
        slow = head
        while fast.next != None:
            fast = fast.next
            slow = slow.next
        slow.next =slow.next.next
        return head
