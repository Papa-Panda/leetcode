class Solution(object):
    def search(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: bool
        """
        start=0
        end=len(nums)-1
        
        while start<=end:
            print(start,end)
            mid=(start+end)/2
            if nums[mid]==target:
                return True
            if nums[mid] == nums[start] == nums[end]:
                start += 1
                end -= 1
            elif nums[mid]>=nums[start]:#当nums[mid]属于左边升序序列时
                if target>=nums[start] and target<=nums[mid]:
                    end=mid-1
                else:
                    start=mid+1
            elif nums[mid]<=nums[end]:#当nums[mid]属于右边升序序列时
                if target>=nums[mid] and target<=nums[end]:
                    start=mid+1
                else:
                    end=mid-1
        return False
