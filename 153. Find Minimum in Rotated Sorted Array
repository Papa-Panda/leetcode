class Solution(object):
    def findMin(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        left, right = 0, len(nums) - 1
        while left < right:
            mid = left + (right - left)/2
            # print(nums[left],nums[mid])
            if nums[mid] < nums[right]:
                right = mid
            else:
                left = mid +1
        return nums[left]
